<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="testingtheapp.rssfeed">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Main2Activity">
            <intent-filter>
                <action android:name="com.testingtheapp.rssfeed.MAINACTIVITY" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".players" />
        <activity android:name=".News" />
        <activity android:name=".Rankings" />
        <activity android:name=".PlayerRankings" />
        <activity android:name=".TeamRankings" />
        <activity
            android:name=".FixturesWorld"
            android:label="@string/title_activity_fixtures_world" />
        <activity
            android:name=".WebList"
            android:label="@string/title_activity_web_list" />
        <activity
            android:name=".Reviews"
            android:label="@string/title_activity_reviews" />
        <activity
            android:name=".Fixture"
            android:label="@string/title_activity_fixture" />
        <activity
            android:name=".MainScreen"
            android:label="@string/title_activity_main_screen"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>
